            $("#maincount").countdown({ until: liftoffTime, format: 'HMS', expiryUrl: "/edulite/Exams/finish/1" });
            function serverTime() {
                var time = null;
                $.ajax({
                    url: "/edulite/ServerTimes",
                    async: false, dataType: 'text',
                    success: function (text) {
                        time = new Date(text);
                    }, error: function (http, message, exc) {
                        time = new Date();
                    }
                });
                return time;
            }
        });
