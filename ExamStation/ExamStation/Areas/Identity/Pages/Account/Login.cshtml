    <link href="~/ExamStation/assets/global/css/plugins.css" rel="stylesheet" type="text/css" />
    <link href="~/ExamStation/assets/admin/layout/css/layout.css" rel="stylesheet" type="text/css" />
    <link href="~/ExamStation/assets/admin/layout/css/themes/darkblue.css" rel="stylesheet" type="text/css" id="style_color" />
